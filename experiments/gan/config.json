{

    "seed" : 42,

    "device" : "cuda",

    "gan_config" : {
        "latent_dim" : 128,
        "channels" : [512,256,128,64],
        "out_channels" : 3,
        "output_activation" : "sigmoid"
    },

    "discriminator_config" : {
        "channels" : [3,64,128,256,512,1],
        "filters" : [4,4,4,4,4],
        "strides" : [2,2,2,2,1],
        "paddings" : [1,1,1,1,0]
    },

    "batch_size" : 16,
    "learning_rate" : 1e-4,
    "total_epochs" : 100,
    "noise_size" : [4,4],
    "loss_fn" : "bce",

    "num_workers" : 4,
    "prefetch_factor" : 2,

    "dataset" : "anime_faces",
    "save_image_every" : 500,
    "grid_size" : 4

}